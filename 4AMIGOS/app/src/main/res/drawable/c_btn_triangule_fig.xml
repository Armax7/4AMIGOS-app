<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <rotate
            android:fromDegrees="-3"
            android:toDegrees="0"
            android:pivotX="0%"
            android:pivotY="100%" >
            <shape
                android:shape="rectangle" >
                <stroke
                    android:color="@color/azulDark4A"
                    android:width="2dp" />
                <solid
                    android:color="@color/azulDark4A" />
            </shape>
        </rotate>
    </item>
</layer-list>